<h1>Latest Place</h1>

<div class= span6 boxy >

	<% if @place.present? %>

		Title: <%= @place.title %>

		<br/>

		Address: <%= @place.address %>

<script src="//maps.google.com/maps?file=api&amp;v=2&amp;key=AIzaSyDzleZt1Cn-Ha1cn19cC8LOKVFueX_iYB4"
            type="text/javascript"></script>

<script type="text/javascript">
         $(function() {

         $( window ).unload(function() {
         	GUnload();
         });


      if (GBrowserIsCompatible()) {
        var map = new GMap2(document.getElementById("map_canvas"));
        map.setCenter(new GLatLng(33.5, 36.3), 13);
 
			var point = new GLatLng(33.5, 36.3);                      
        	map.addOverlay(new GMarker(point));
          
        }
    }

});

    </script>

    <div id="map_canvas" style="width: 500px; height: 300px"></div>

	<% else %>
	<h2> Enter a Place, because we don't have any </h2>
	<% end %>
<div>

<iframe width="420" height="315" src="//www.youtube.com/embed/KpRHRh9PaRU" frameborder="0" allowfullscreen></iframe>